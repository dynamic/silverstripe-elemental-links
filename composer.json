{
    "name": "dynamic/silverstripe-elemental-links",
    "description": "Links block for SilverStripe Elemental",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "blocks",
        "dynamic",
        "links"
    ],
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "https://www.dynamicagency.com"
        }
    ],
    "require": {
        "dnadesign/silverstripe-elemental": "^4.4",
        "gorriecoe/silverstripe-linkfield": "^1.0",
        "symbiote/silverstripe-gridfieldextensions": "^3.2"
    },
    "require-dev": {
        "silverstripe/recipe-testing": "^2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dynamic\\Elements\\Links\\": "src/",
            "Dynamic\\Elements\\Links\\Test\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true,
            "silverstripe/recipe-plugin": true
        },
        "process-timeout": 600
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
