{
    "name": "dynamic/silverstripe-elemental-links",
    "type": "silverstripe-vendormodule",
    "description": "Links block for SilverStripe Elemental",
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "http://www.dynamicagency.com"
        }
    ],
    "keywords": [
        "silverstripe",
        "elemental",
        "blocks",
        "dynamic",
        "links"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/vendor-plugin": "^1.0",
        "dnadesign/silverstripe-elemental": "^4.4",
        "dynamic/silverstripe-linkable": "^1.0",
        "symbiote/silverstripe-gridfieldextensions": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "process-timeout": 600
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Elements\\Links\\": "src/",
            "Dynamic\\Elements\\Links\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
