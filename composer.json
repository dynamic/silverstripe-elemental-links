{
    "name": "dynamic/silverstripe-elemental-links",
    "description": "Links block for SilverStripe Elemental",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "blocks",
        "dynamic",
        "links"
    ],
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "https://www.dynamicagency.com"
        }
    ],
    "require": {
        "dnadesign/silverstripe-elemental": "^5",
        "silverstripe/linkfield": "^4.0",
        "symbiote/silverstripe-gridfieldextensions": "^4"
    },
    "require-dev": {
        "silverstripe/recipe-testing": "^3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dynamic\\Elements\\Links\\": "src/",
            "Dynamic\\Elements\\Links\\Test\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        },
        "process-timeout": 600
    }
}
